<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:MondialMunchGUI.ViewModels"
             x:DataType="vm:ViewRecipeViewModel"
             mc:Ignorable="d" d:DesignWidth="250" d:DesignHeight="450"
             x:Class="MondialMunchGUI.Views.ViewRecipeView"
             x:CompileBindings="True">
             
  
    <StackPanel>
        <TextBlock Text="{Binding Name, DataType={x:SpecificRecipe vm:ViewRecipeViewModel}}"></TextBlock>
    </StackPanel>
</UserControl>
